{
  "name": "marksyncr",
  "version": "0.5.2",
  "private": true,
  "description": "Cross-browser bookmark sync extension with cloud storage",
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "version:bump": "tsx scripts/bump-version.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.17.0",
    "globals": "^17.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.21.0",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=24.0.0"
  }
}
